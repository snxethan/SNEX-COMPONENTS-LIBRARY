{
  "name": "@snxethan/snex-components",
  "version": "0.2.0",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "tailwind-preset.cjs",
    "styles.css"
  ],
  "dependencies": {
    "lucide-react": "^0.470.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.2.1"
  },
  "peerDependencies": {
    "react": ">=18 <21",
    "react-dom": ">=18 <21",
    "next": ">=13",
    "tailwindcss": ">=3.3",
    "postcss": ">=8.4",
    "autoprefixer": ">=10.4"
  },
  "devDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.6.0",
    "tsup": "^8.0.0",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./tailwind-preset": "./tailwind-preset.cjs",
    "./styles.css": "./styles.css"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "postinstall": "node -e \"console.log('[snex-components] Add our preset + content (TW v3) or @source (TW v4). See README.')\""
  }
}
